{
  "collectCoverageFrom": [
    "**/src/**.{js,jsx}",
    "!**/src/entry.js",
    "!**/src/entry.*.js",
    "!**/src/pdf.worker.entry.js"
  ],
  "moduleNameMapper": {
    "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
    "pdfjs-dist/(.*)": "pdfjs-dist/legacy/$1"
  },
  "setupFilesAfterEnv": ["jest-canvas-mock", "@testing-library/jest-dom"],
  "testEnvironment": "<rootDir>/jest.env.js"
}
